[global]
interface    127.0.0.1
port        19333

[device]
name        arduino
output      /dev/ttyACM0
channels    9
type        momo
interval    20000
rate        38400
prefix      55 AA

#arduino bootloader runs when opening the serial port for the first time
#delay transmission one second after opening so we don't send shit to the bootloader
delayafteropen  1000000
#debug        on

[color]
name        red
rgb         FF0000
adjust      1

[color]
name        green
rgb         00FF00
adjust      1

[color]
name        blue
rgb         0000FF
adjust      0.4

[light]
name         right
color        red     arduino  2
color        green   arduino  3
color        blue    arduino  1
hscan        75 100
vscan        0 100

[light]
name         left
color        red     arduino  5
color        green   arduino  6
color        blue    arduino  4
hscan        0 25
vscan        0 100

[light]
name         top
color        red     arduino  8
color        green   arduino  9
color        blue    arduino  7
hscan        0 100
vscan        0 25